#include<stdio.h>
int main()
{
    char c;
    printf("Enter a character:");
    scanf("%c", &c);
    if ((c>=65 && c<=90) || (c>=97 && c<=122))      // ASCII VALUE
    {
    switch(c)
    {
        case 'a':
        case 'e':
        case 'i':
        case 'o':
        case 'u':
    printf("%c is a vowel", c);
    break;
    default:
    printf("%c is consonant", c);
    break;
    }
    }
    else{
        printf("%c not an alphabet", c);
    }
    return 0;
}
